    <script src="//cdn.polyfill.io/v2/polyfill.min.js"></script>
    <% if(env !== 'development'){ %>
      <link rel="stylesheet" href="/styles.css">
    <% } %>
    <!-- Custom styles for this template -->
    <style>
      body {
        padding-top: 50px;
      }
      .starter-template {
        padding: 40px 15px;
        text-align: center;
      }
